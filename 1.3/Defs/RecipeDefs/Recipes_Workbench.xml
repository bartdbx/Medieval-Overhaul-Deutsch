<!-- Ammuniation: Stone Boulder --> 
<RecipeDef ParentName="DankPyon_MakeableAmmunitionBase">
	<defName>DankPyon_MakeBullet_StoneBoulder</defName>
	<workAmount>800</workAmount>		
	<label>Steinbrocken in Steinblöcke schneiden</label>
	<jobString>Steinbrocken in Steinblöcke schneiden.</jobString>
	<targetCountAdjustment>5</targetCountAdjustment>	
    <researchPrerequisites>
        <li>DankPyon_Trebuchet</li>
    </researchPrerequisites>
	<ingredients>
		<li>
			<filter>
				<categories>
            <li>StoneChunks</li>
        </categories>
			</filter>
			<count>1</count>
		</li>
	</ingredients>
	<fixedIngredientFilter>
		<categories>
        <li>StoneChunks</li>
    </categories>
	</fixedIngredientFilter>
	<products>
		<DankPyon_StoneBoulder>1</DankPyon_StoneBoulder>
	</products>		
</RecipeDef>

